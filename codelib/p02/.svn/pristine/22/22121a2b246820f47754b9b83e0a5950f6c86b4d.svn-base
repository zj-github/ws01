package cn.innohub.crawler.beans;

public class ExtractField {
	private String name;// 此规则的名称
	private String method;// 调用哪个类.方法 //默认为Jsoup的select
	private String reg;// 表达式
	public ExtractField() {
	}
	public ExtractField(String name,String method,String reg) {
		this.name = name;
		this.method = method;
		this.reg = reg;
	}
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getMethod() {
		return method;
	}

	public void setMethod(String method) {
		this.method = method;
	}

	public String getReg() {
		return reg;
	}

	public void setReg(String reg) {
		this.reg = reg;
	}
	@Override
	public String toString() {
		return "ExtractField [name=" + name + ", method=" + method + ", reg=" + reg + "]";
	}
	
}
