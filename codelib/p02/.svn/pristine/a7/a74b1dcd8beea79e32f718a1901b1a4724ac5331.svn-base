<?xml version="1.0" encoding="UTF-8"?>
<!-- 每个主机对应的抽取规则 -->
<extract-rules>
	<rule>
		<hostname>www.renrentou.com</hostname>
		<clazz>cn.innohub.crawler.beans.innohub.CCFAProjectInfo</clazz>
		<field>
			<name>projectName</name>
			<descript>项目名称</descript>
			<method></method>
			<element>div.r-text1</element>
		</field>
		<field>
			<name>projectLogo</name>
			<descript>项目Logo地址</descript>
			<method></method>
			<element>div.details-l-img > img</element>
			<attr>src</attr>
		</field>
		<field>
			<name>description</name>
			<descript>项目描述</descript>
			<method></method>
			<element>div.attention-list > p ~ p</element>
		</field>
		
<!-- 		<field>
			<name>city</name>
			<descript>所在城市</descript>
			<method></method>
			<element>div.attention-list > p@1</element>
		</field> -->
<!-- 		<field>
			<name>isfinance</name>
			<descript>是否融资（0,开始融资1,没有融资2,融资完成)</descript>
			<method></method>
			<element>h1.fw16</element>
		</field> -->

		<field>
			<name>contactName</name>
			<descript>联系人姓名</descript>
			<method></method>
			<element>dl.attention-dl>dd>b</element>
		</field>
	</rule>
</extract-rules>